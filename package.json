{
  "name": "pw-generator-ext",
  "displayName": "PW Generator",
  "version": "1.0.0",
  "author": "Bhavik Jikadara",
  "description": "This Chrome extension, built with ReactJS, helps users create strong, customizable passwords and random word combinations. It lets you adjust password strength, length, and character types, plus generate random words for brainstorming or creativity.",
  "type": "module",
  "license": "MIT",
  "keywords": [
    "chrome-extension",
    "react",
    "vite",
    "create-chrome-ext"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "vite build --mode development --watch",
    "build": "vite build",
    "preview": "vite preview",
    "fmt": "prettier --write '**/*.{jsx,js,json,css,scss,md}'",
    "zip": "npm run build && node src/zip.js"
  },
  "dependencies": {
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-toast": "^1.2.3",
    "clsx": "^2.1.1",
    "crypto-js": "^4.2.0",
    "firebase": "^10.14.1",
    "lucide-react": "^0.468.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.11.2",
    "tailwind-merge": "^2.5.5"
  },
  "devDependencies": {
    "@crxjs/vite-plugin": "2.0.0-beta.18",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.16",
    "terser": "^5.37.0",
    "vite": "^4.3.9"
  }
}
